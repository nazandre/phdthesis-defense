  ISSN                     = {0018-9545},
  Keywords                 = {cognitive radio;game theory;graph theory;on-board communications;peer-to-peer computing;radio broadcasting;vehicular ad hoc networks;CR;OBU;P2P approach;RSU;V2R broadcasting;V2V transmissions;coalition formation algorithm;coalitional graph game;cognitive radio VANET;content distribution;data exchange;data request;file packets;missing packets;onboard units;peer-to-peer approach;roadside units;vehicle-to-roadside broadcasting;vehicle-to-vehicle transmissions;vehicular ad hoc networks;Cognitive radio;Games;Heuristic algorithms;Peer to peer computing;Protocols;Silicon;Vehicles;Coalitional graph game;popular content distribution;vehicular ad hoc networks},
  Owner                    = {andre},
