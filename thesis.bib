@InProceedings{Yoon-msreport07,
  Title                    = {Power Delivery Circuit for Scalable Claytronics},
  Author                   = {Yoon, Byung Woo},
  Booktitle                = {MSc Report, Carnegie Mellon University, Department of
 Electrical and Computer Engineering},
  Year                     = {2007},
  Month                    = {May},

  Abstract                 = {Catoms are elements of physically reconfigurable matter that can be programmed to assemble and emulate 3D physical objects. The challenge is to deliver power to an ensemble of catoms, which may number from tens to millions of catoms in depth. This work explores AC power transfer through the capacitive interfaces of the catom. 
 Three methods of power transfer are explored: ?Continuous?, ?Switching? and ?Store and Send? power transfer. The Store and Send approach provides charge time scalability independent of the power consumption of the catom, while the charge voltage is also independent of the power consumption of the catom. This is done by alternating between store and send: while the charge time for each catom is constant, the total charge time increases
as the square of the number of catom in series. The charge time and the charge voltage for the Continuous and Switching methods scale with the number of catoms, but depend on the power consumption of the catom.
Charge time and the transfer efficiency for Store and Send power transfer are validated using analysis, simulation, and experiments with discrete components. The experiments prove that a square wave input of 10 V peak to peak, at 1 MHz, through a capacitive interface of 1 nF transfers charge to a 2 mF capacitor in 6 seconds up to a voltage of 4.5V. Using a capacitive interface of 20 nF the charge time reduces to 0.8 seconds. The best measured power consumption in the catom is 10.8 mW, corresponding to an efficiency of 45\% for the 20 nF catom interface. The transfer efficiency improves by reducing the power consumption of the circuits and by increasing the area of the capacitive interface of the catom.},
  Also                     = {Electrostatic Latching for Inter-module Adhesion, Power
 Transfer, and Communication in Modular Robots in IROS '07},
  Keywords                 = {Actuation, Adhesion, Power Routing},
  Owner                    = {andre},
  Timestamp                = {2017.06.28},
  Url                      = {www.cs.cmu.edu/~claytronics/papers/yoon-msreport07.pdf}
}

