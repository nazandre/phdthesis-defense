@InProceedings{ldp,
  Title                    = {Programming Modular Robots with Locally Distributed Predicates},
  Author                   = {De~Rosa, Michael and Goldstein, Seth Copen and Lee, Peter and Campbell, Jason D. and Pillai, Padmanabhan},
  Booktitle                = {Proceedings of the IEEE International Conference on Robotics and Automation {ICRA '08}},
  Year                     = {2008},

  Abstract                 = {We present a high-level language for programming modular robotic systems, based on locally distributed predicates (LDP), which are distributed conditions that hold for a connected subensemble of the robotic system. An LDP program is a collection of LDPs with associated actions which are triggered on any subensemble that matches the predicate. The result is a reactive programming language which efficiently and concisely supports ensemble-level programming. We demonstrate the utility of LDP by implementing three common, but diverse, modular robotic tasks.},
  Keywords                 = {Claytronics, Modular Robotics, Programming, LDP},
  Owner                    = {bourgeois},
  Timestamp                = {2011.01.04},
  Venue                    = {IEEE International Conference on Robotics and Automation (ICRA)}
}

@Article{mderosa-ijrr-2008,
  Title                    = {Distributed Watchpoints: Debugging Large Modular Robotic Systems},
  Author                   = {De~Rosa, Michael and Goldstein, Seth Copen and Lee, Peter and Campbell, Jason D. and Pillai, Padmanabhan},
  Journal                  = {International Journal of Robotics Research},
  Year                     = {2008},

  Month                    = {March},
  Note                     = {Special Issue on Modular Robotics},
  Number                   = {3},
  Volume                   = {27},

  Abstract                 = {Distributed systems frequently exhibit properties of interest which span multiple entities. These properties cannot easily be detected from any single entity, but can be readily be detected by combining the knowledge of multiple entities. Testing for distributed properties is especially important in debugging or verifying software for modular robots. We have developed a technique we call distributed watchpoint triggers which can efficiently recognize distributed conditions. Our watchpoint description language can handle a variety of temporal, spatial, and logical properties spanning multiple robots. This paper presents the specification language, describes the distributed online mechanism for detecting distributed conditions in a running system, and evaluates the performance of our implementation.},
  Also                     = {Distributed Watchpoints: Debugging Large Multi-Robot Systems, (icra07)},
  Keywords                 = {Debugging, Distributed Systems},
  Owner                    = {andre},
