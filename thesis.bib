  Doi                      = {10.1109/TrustCom.2011.154},
  Keywords                 = {automotive engineering;control engineering computing;distributed control;embedded systems;operating systems (computers);synchronisation;time division multiple access;FlexRay;TDMA protocol;clock synchronization;distributed embedded control system;distributed real-time operating system;embedded system;intertask synchronization;location-transparent system call;real-time control application program;task management;time division multiple access;Automotive engineering;Control systems;Delay;Operating systems;Real time systems;Synchronization;Time factors;automotive electronics;distributed systems;embedded systems;operating systems;real-time systems},
  Owner                    = {andre},
  Timestamp                = {2017.06.28}
}

@InProceedings{chiba2013distributed,
  Title                    = {A Distributed Real-Time Operating System with Distributed Shared Memory for Embedded Control Systems},
  Author                   = {Chiba, Takahiro and Yoo, Myungryun and Yokoyama, Takanori},
  Booktitle                = {Dependable, Autonomic and Secure Computing (DASC), 2013 IEEE 11th International Conference on},
  Year                     = {2013},
  Organization             = {IEEE},
  Pages                    = {248--255},

  Abstract                 = {The paper presents a distributed real-time operating system (DRTOS) that provides a distributed shared memory (DSM) service for distributed control systems. Model-based design has become popular in embedded control software design and the source code of software modules can be generated from a controller model. The generated software modules exchange their input and output values through shared variables. We develop a DRTOS with a real-time DSM service to provide a location-transparent environment, in which distributed software modules can exchange input and output values through the DSM. The DRTOS is an extension to OSEK OS. We use a real-time network called FlexRay, which is based on a TDMA (Time Division Multiple Access) protocol. The consistency of the DSM is maintained according to the order of data transfer through FlexRay, not using inter-node synchronization. The worst case response time of the DSM is predictable if the FlexRay communication is well configured.},
