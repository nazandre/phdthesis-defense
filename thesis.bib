}

@InProceedings{kirby05,
  Title                    = {Catoms: Moving Robots Without Moving Parts},
  Author                   = {Kirby, Brian and Campbell, Jason D. and Aksak, Burak and Pillai, Padmanabhan and Hoburg, James F. and Mowry, Todd C. and Goldstein, Seth Copen},
  Booktitle                = {AAAI (Robot Exhibition)},
  Year                     = {2005},

  Address                  = {Pittsburgh, PA},
  Month                    = {July},
  Pages                    = {1730--1},

  Abstract                 = {We demonstrate modular robot prototypes developed as part of the Claytronics Project (Goldstein et al. 2005). Among the novel features of these robots (�catoms�) is their ability to reconfigure (move) relative to one another without moving parts. The absence of moving parts is central to one key aim of our work, namely, plausible manufacturability at smaller and smaller physical scales using high-volume, low-unit-cost techniques such as batch photolithography, multi-material submicron 3D lithographic processing, and self assembly. Claytronics envisions multi-million-module robot ensembles able to form into three dimensional scenes, eventually with sufficient fidelity so as to convince a human observer the scenes are real. This work presents substantial challenges in mechanical and electronic design, control, programming, reliability, power delivery, and motion planning (among other areas), and holds the promise of radically altering the relationship between computation, humans, and the physical world.},
  Keywords                 = {Ensemble Principle},
  Owner                    = {andre},
  Timestamp                = {2017.06.28},
  Url                      = {http://www.cs.cmu.edu/~claytronics/papers/kirby05.pdf},
